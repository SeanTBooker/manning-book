{
    "resource": [
        {
            "aws_vpc": [
                {
                    "hello-world-network": [
                        {
                            "cidr_block": "10.0.0.0/16",
                            "enable_dns_hostnames": true,
                            "enable_dns_support": true,
                            "name": "hello-world-network"
                        }
                    ]
                }
            ]
        },
        {
            "aws_subnet": [
                {
                    "hello-world-network": [
                        {
                            "cidr_block": "10.0.0.0/16",
                            "vpc_id": "${aws_vpc.aws.hello-world-network}"
                        }
                    ]
                }
            ]
        },
        {
            "aws_internet_gateway": [
                {
                    "hello-world-network": [
                        {
                            "vpc_id": "${aws_vpc.aws.hello-world-network}"
                        }
                    ]
                }
            ]
        },
        {
            "aws_vpn_gateway": [
                {
                    "hello-world-vpn": [
                        {
                            "vpc_id": "${aws_vpc.aws.hello-world-network}"
                        }
                    ]
                }
            ]
        },
        {
            "aws_customer_gateway": [
                {
                    "hello-world-vpn": [
                        {
                            "bgp_asn": 65000,
                            "ip_address": "10.2.0.0/16",
                            "type": "ipsec.1"
                        }
                    ]
                }
            ]
        },
        {
            "aws_vpn_connection": [
                {
                    "hello-world-vpn": [
                        {
                            "customer_gateway_id": "${aws_customer_gateway.aws-cgw.id}",
                            "static_routes_only": false,
                            "type": "ipsec.1",
                            "vpn_gateway_id": "${aws_vpn_gateway.aws-vpn-gw.id}"
                        }
                    ]
                }
            ]
        }
    ]
}