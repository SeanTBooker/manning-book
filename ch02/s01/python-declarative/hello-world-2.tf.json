{
    "resource": [
        {
            "google_compute_instance": [
                {
                    "hello-world-2": [
                        {
                            "allow_stopping_for_update": true,
                            "boot_disk": [
                                {
                                    "initialize_params": [
                                        {
                                            "image": "ubuntu-1804-lts"
                                        }
                                    ]
                                }
                            ],
                            "machine_type": "f1-micro",
                            "name": "hello-world-2",
                            "network_interface": [
                                {
